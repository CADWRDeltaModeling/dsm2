#runtime
START_DATE      	28JUN1996
START_TIME              0600
#qual
QUAL_START_DATE      	14JUL1996
PTM_START_DATE          ${QUAL_START_DATE}
END_DATE        	01SEP1996
END_TIME                0000
FINE_OUT                1HOUR



# Input envvars
STUDYDIR                .
TSINPUTDIR              ./timeseries

#BNDRYINPUT              ${TSINPUTDIR}/hydro-merged-9002.dss	# Historical boundary input
BNDRYINPUT              ${TSINPUTDIR}/hist19902005.dss

GATEFILE		${TSINPUTDIR}/gates_db_200611.dss			# Gates DSS file


DICUFILE	        ${TSINPUTDIR}/dicu.dss			# DICU flow file
