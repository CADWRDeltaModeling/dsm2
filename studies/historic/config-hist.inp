# Configuration.inp
# Define variables and file names here

ENVVARS
NAME			VALUE
BINDIR                  ..\..\..\bin
HYDROEXE		hydro.exe
QUALEXE			qual.exe

SIMNAME		        historical		# name of simulation
DSM2MODIFIER		hist    		# Study name used for DSM2 output
DSM2AGENCY		dwr
HISTFLOWVERSION         DWR-DMS-200512
HISTSTAGEVERSION        ${HISTFLOWVERSION}
