ENVVAR
NAME              VALUE
DSM2MODIFIER        historical            # Study name used for DSM2 output
DSM2INPUTDIR        ../../common_input
DSM2AGENCY          dwr
VERSIONDATE         200705
HISTFLOWVERSION     DWR-DMS-${VERSIONDATE}
HISTSTAGEVERSION    ${HISTFLOWVERSION}
HISTQUALVERSION     DWR-DMS-${VERSIONDATE}

#runtime
START_DATE          01JAN1998
START_TIME          0000
QUAL_START_DATE     02JAN1998
PTM_START_DATE      ${QUAL_START_DATE}
END_DATE            01MAR1998
END_TIME            0000
TEMPDIR             z:/temp          # Spacious directory for scratch files

# Input envvars
STUDYDIR        .
TSINPUTDIR      ../../timeseries
BNDRYINPUT      ${TSINPUTDIR}/hist_19902007.dss 
GATEFILE        ${TSINPUTDIR}/gates_db.dss        
DICUFILE        ${TSINPUTDIR}/dicu_${VERSIONDATE}.dss            
QUALBNDINP          ${TSINPUTDIR}/hist_19902007.dss
DICUFILE_ECS        ${TSINPUTDIR}/dicuwq_200611.dss        # seasonal DICU file for EC 
DICUFILE_ECE        ${TSINPUTDIR}/dicuwq_200611_expand.dss  # expanded DICU file for EC 
DICUFILE-ECS        ${TSINPUTDIR}/dicu.dss                # seasonal DICU file for EC 
CLIMATE             ${TSINPUTDIR}/climate-5years.dss
STOCKTON_FLOW_FILE   constant      # needed for DO runs, if not available use constant
STOCKTON_EFF_FLOW    0.0           # needed for DO runs, if not available use 0.0
# Output
DSM2OUTPUTDIR       ${STUDYDIR}/output
OUTPUTFILE          ${DSM2MODIFIER}.dss
# hydro
FLOWPATTERN         ./output/FLOWPATTERN.dss
HYDROOUTDSSFILE     ${DSM2OUTPUTDIR}/${OUTPUTFILE}
HYDROOUTFILE          ${DSM2OUTPUTDIR}/${DSM2MODIFIER}.hof
HYDRORSTFILE        ${DSM2OUTPUTDIR}/${DSM2MODIFIER}.hrf
HYDROTIDEFILE          ${DSM2OUTPUTDIR}/${DSM2MODIFIER}.h5
GATEOUTPUTFILE      ${DSM2OUTPUTDIR}/${OUTPUTFILE}
# Qual
QUALOUTDSSFILE      ${DSM2OUTPUTDIR}/${OUTPUTFILE}
QUALOUTFILE          ${DSM2OUTPUTDIR}/${DSM2MODIFIER}.qof
QUALRSTFILE          ${DSM2OUTPUTDIR}/${DSM2MODIFIER}.qrf
FINE_OUT            15MIN
# Jones Tract Levee Break 
EVENTSINPUT             ${TSINPUTDIR}/events.dss
END

SCALAR
NAME            VALUE                                                        
title           "Historical SIMULATION ${DSM2MODIFIER}" 
checkdata       false                               
cont_bad        false                               
cont_missing    true                                
cont_question   true                                
cont_unchecked  true                                
deltax          5000                                
display_intvl   1day                                
flush_output    15day                               
gravity         32.2                                
hydro_time_step 15min                               
levee_slope     0.33                                
maxiter         20                                  
printlevel      1                                   
theta           0.6                                 
toleranceq      0.001                               
tolerancez      0.0008                              
warn_missing    true                                
warn_question   true                                
warn_unchecked  false
run_start_date   ${START_DATE}
run_end_date     ${END_DATE}
run_start_time   ${START_TIME}
run_end_time     ${END_TIME}
temp_dir         c:/temp                             
END

IO_FILE
MODEL TYPE    IO  INTERVAL FILE               
hydro hdf5    out    1hour ${DSM2MODIFIER}.h5  #Flow data to be passed to qual and ptm 
hydro restart out     1mon ${DSM2MODIFIER}.hrf #Initial condition a restart is required
hydro output  out     none ${DSM2MODIFIER}.out #Echoed run status                      
hydro echo    out     none ${DSM2MODIFIER}_hydro_echo.inp  #Echoed input                              
END

PARAMETER
${DSM2INPUTDIR}/scalar_hydro_std.inp
END

GRID
${DSM2INPUTDIR}/channel_std_delta_grid.inp
${DSM2INPUTDIR}/gate_std_delta_grid.inp
${DSM2INPUTDIR}/reservoir_std_delta_grid.inp
${DSM2INPUTDIR}/channel_historical_stability_revision.inp
END

INITIAL_CONDITION
${DSM2INPUTDIR}/channel_ic_std_delta_grid.inp
${DSM2INPUTDIR}/reservoir_ic_std_delta_grid.inp
END

HYDRO_TIME_SERIES
${DSM2INPUTDIR}/boundary_flow_delta_historical.inp
${DSM2INPUTDIR}/source_flow_delta_historical.inp
${DSM2INPUTDIR}/boundary_stage_delta_historical.inp
${DSM2INPUTDIR}/source_flow_dicu_historical.inp
${DSM2INPUTDIR}/boundary_flow_jones_hydro.inp
END

OPERATION
${DSM2INPUTDIR}/oprule_historical_gate_ops.inp
${DSM2INPUTDIR}/oprule_hist_temp_barriers_op.inp
END

OUTPUT_TIME_SERIES
${DSM2INPUTDIR}/output_channel_std_hydro_rki.inp
${DSM2INPUTDIR}/output_channel_std_hydro_named.inp
${DSM2INPUTDIR}/output_channel_std_hydro_boundary.inp
END

