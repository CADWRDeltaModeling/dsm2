MIDR_TRIGGER_FLOW                                    4000                                                                                               
MIDR_STAGE_LOW                                       midr_stage_low                                                                                     
OLDR_FLOOD                                           oldr_flood                                                                                         
OLDR_STAGE_OP                                        oldr_op                                                                                            
OLDR_STAGE_RELAX                                     oldr_stage_relax                                                                                   
OLDR_UNIDIR                                          oldr_unidir                                                                                        
