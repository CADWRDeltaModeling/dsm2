
ENVVAR
NAME            VALUE     
HYDROOUTDSSFILE output.dss 
DSM2MODIFIER    channel    
END



SCALAR
NAME            VALUE                              
title           "TUTORIAL SIMULATION ${DSM2MODIFIER}" 
checkdata       false                               
cont_bad        false                               
cont_missing    true                                
cont_question   true                                
cont_unchecked  true                                
deltax          5000                                
display_intvl   1day                                
flush_output    15day                               
gravity         32.2                                
hydro_time_step 15min                               
levee_slope     0.33                                
maxiter         20                                  
printlevel      1                                   
theta           0.6                                 
toleranceq      0.001                               
tolerancez      0.0008                              
warn_missing    true                                
warn_question   true                                
warn_unchecked  false                               
END



IO_FILE
MODEL TYPE    IO  INTERVAL FILE               
hydro hdf5    out    1hour ${DSM2MODIFIER}.h5  #Flow data to be passed to qual and ptm 
hydro restart out     1mon ${DSM2MODIFIER}.hrf #Initial condition a restart is required
hydro output  out     none ${DSM2MODIFIER}.out #Echoed run status                      
hydro echo    out     none echo.inp            #Echoed input                           
END


