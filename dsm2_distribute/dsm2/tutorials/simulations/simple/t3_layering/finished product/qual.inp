
ENVVAR
NAME           VALUE     
QUALOUTDSSFILE output.dss 
DSM2MODIFIER   layering   
END



SCALAR
NAME           VALUE                              
title          "TUTORIAL SIMULATION ${DSM2MODIFIER}" 
run_start_date 01JAN1992                           
run_end_date   01MAR1992                           
run_start_time 0100                                
run_end_time   0000                                
temp_dir       c:/temp                             
checkdata      false                               
cont_bad       false                               
cont_missing   false                               
cont_question  false                               
cont_unchecked true                                
dispersion     true                                
display_intvl  1day                                
flush_output   8day                                
init_conc      500                                 
printlevel     1                                   
qual_time_step 15min                               
warn_missing   false                               
warn_question  false                               
warn_unchecked false                               
END

IO_FILE
MODEL      TYPE     IO                 INTERVAL FILE                    
qual       restart  out                1mon     ${DSM2MODIFIER}.qrf      #Initial condition a restart is required
qual       output   none               none     ${DSM2MODIFIER}_qual.out #Echoed processed input, run status     
qual       echo     out                none     echo.inp                 #Echoed input file                      
END
                
TIDEFILE           
START_DATE END_DATE FILE                       
runtime    length   ${DSM2MODIFIER}.h5         
END


QUAL_TIME_SERIES
input_node_conc_tutorial.inp
END


OUTPUT_TIME_SERIES
output_channel_ec_tutorial.inp
END
