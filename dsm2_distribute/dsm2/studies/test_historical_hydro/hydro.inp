CONFIGURATION
config_hist.inp
END

SCALAR
NAME        VALUE
title            "HISTORICAL SIMULATION ${DSM2MODIFIER}"
checkdata        false
cont_bad        false
cont_missing        true
cont_question        true
cont_unchecked        true
deltax        5000
display_intvl        1day
flush_output        15day
gravity        32.2
hydro_time_step        15min
levee_slope        0.33
maxiter        20
printlevel        1
theta        0.6
toleranceq        0.001
tolerancez        0.0008
warn_missing        true
warn_question        true
warn_unchecked        false
run_start_date   ${START_DATE}
run_end_date     ${END_DATE}
run_start_time      ${START_TIME}
run_end_time     ${END_TIME}
temp_dir     ${TEMPDIR}
END

IO_FILE
MODEL          TYPE       IO     INTERVAL        FILE
hydro          hdf5       out    1hour           ${HYDROTIDEFILE}     # Flow data to be passed to qual and ptm
hydro          restart    out    1day            ${HYDRORSTFILE}      # Initial condition a restart is required
hydro          output     out    none            ${HYDROOUTFILE}      # Echoed input and run status
END 

HYDRO_TIME_SERIES
boundary_flow_delta_historical.inp
source_flow_delta_historical.inp
boundary_stage_delta_historical.inp
source_flow_dicu_historical.inp
boundary_flow_jones_hydro.inp
END

INITIAL_CONDITION
channel_ic_std_delta_grid.inp
reservoir_ic_std_delta_grid.inp
END

GRID
channel_std_delta_grid.inp
gate_std_delta_grid.inp
reservoir_std_delta_grid.inp
END

OPERATION
oprule_historical_gate_ops.inp
oprule_hist_temp_barriers_op.inp
oprule_montezuma_gate_op.inp
oprule_historical_op_improvements.inp
END

OUTPUT_TIME_SERIES
output_channel_std_hydro_rki.inp
output_channel_std_hydro_named.inp
output_reservoir_std_hydro_named.inp
output_channel_std_hydro_boundary.inp
END

