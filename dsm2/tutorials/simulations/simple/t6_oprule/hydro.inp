# Input system for DSM2-HYDRO

ENVVARS
NAME          VALUE
SIMNAME	      tutorial
HYDROOUTDSSFILE	output.dss
DSM2MODIFIER  oprule
TUTORIALINPUT	..\timeseries\tutorial.dss
STAGE_CRITICAL	1.4
STAGE_RELAX 1.6
END

TITLES
DSM2 TUTORIAL SIMULATION: $DSM2MODIFIER  
END

SCALAR
database	dsm2input_access
model_name      ${SIMNAME}_hydro
run_start_date  01JAN1992
run_end_date    01MAR1992
run_start_time	0000
run_end_time	0000
temp_dir	c:/temp
END

IO_FILES 
MODEL	TYPE	IO   INTERVAL	FILENAME
hydro	hdf5	out  1hour	$DSM2MODIFIER.h5       # Flow data to be passed to qual and ptm
hydro	restart	out  1mon	$DSM2MODIFIER.hrf      # Initial condition a restart is required
output	none	none none	$DSM2MODIFIER.out      # Echoed input and run status
END


# Additional text-designated output

OUTPUTPATHS     
NAME    CHAN     DIST        TYPE  INTERVAL    PERIOD      FILENAME
ds_gate   2      0           stage 15min       inst  ${HYDROOUTDSSFILE}
us_gate   1      length      stage 15min       inst  ${HYDROOUTDSSFILE}
ds_gate   2       0           flow  15min       inst ${HYDROOUTDSSFILE}
ds_res    4      0           flow  15min       inst ${HYDROOUTDSSFILE} 
trigger   4      7500         stage 15min       inst ${HYDROOUTDSSFILE} 
END

 

OUTPUTPATHS     
NAME    RESERVOIR            TYPE  INTERVAL    PERIOD      FILENAME
res_1    res_1                stage 15min       inst  ${HYDROOUTDSSFILE}
END

