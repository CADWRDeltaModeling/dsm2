
ENVVAR
NAME           VALUE     
OUTPUT         output.inp 
DSM2MODIFIER   channel    
QUALOUTDSSFILE output.dss 
END



SCALAR
NAME           VALUE                              
title          "TUTORIAL SIMULATION ${DSM2MODIFIER}" 
run_start_date 01JAN1992                           
run_end_date   01MAR1992                           
run_start_time 0100                                
run_end_time   0000                                
temp_dir       c:/temp                             
checkdata      false                               
cont_bad       false                               
cont_missing   false                               
cont_question  false                               
cont_unchecked true                                
dispersion     true                                
display_intvl  1day                                
flush_output   8day                                
init_conc      500                                 
printlevel     1                                   
qual_time_step 15min                               
warn_missing   false                               
warn_question  false                               
warn_unchecked false                               
END



IO_FILE
MODEL      TYPE     IO    INTERVAL FILE                    
qual       restart  out   1mon     ${DSM2MODIFIER}.qrf      #Initial condition or a restart is required
qual       output   none  none     ${DSM2MODIFIER}_qual.out #processed data, run status                
qual       echo     out   none     ${DSM2MODIFIER}_qual_echo.inp  #Echoed input         
END
                
TIDEFILE           
START_DATE END_DATE FILE                       
runtime    length   ${DSM2MODIFIER}.h5         
END



NODE_CONCENTRATION
NAME             NODE_NO VARIABLE FILLIN FILE       PATH
upstream_flow          1 ec       last   constant   200. 
downstream_stage       7 ec       last   constant 30000. 
END



OUTPUT_CHANNEL
NAME         CHAN_NO DISTANCE VARIABLE INTERVAL PERIOD_OP FILE             
bnd_1              1        0 ec          15min inst      ${QUALOUTDSSFILE} 
bnd_6              6   length ec          15min inst      ${QUALOUTDSSFILE} 
chan2_half         2     7500 ec          15min inst      ${QUALOUTDSSFILE} 
chan2_length       2   length ec          15min inst      ${QUALOUTDSSFILE} 
chan2004        2004        0 ec          15min inst      ${QUALOUTDSSFILE} 
END


