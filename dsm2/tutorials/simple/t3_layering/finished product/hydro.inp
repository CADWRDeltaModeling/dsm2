
ENVVAR
NAME            VALUE     
HYDROOUTDSSFILE output.dss 
DSM2MODIFIER    layers_input_blocks    
END



SCALAR
NAME            VALUE                              
run_start_date  01JAN1992                           
run_end_date    01MAR1992                           
run_start_time  0000                                
run_end_time    0000                                
temp_dir        c:/temp                             
title           "TUTORIAL SIMULATION ${DSM2MODIFIER}" 
checkdata       false                               
cont_bad        false                               
cont_missing    true                                
cont_question   true                                
cont_unchecked  true                                
deltax          5000                                
display_intvl   1day                                
flush_output    15day                               
gravity         32.2                                
hydro_time_step 15min                               
levee_slope     0.33                                
maxiter         20                                  
printlevel      1                                   
theta           0.6                                 
toleranceq      0.001                               
tolerancez      0.0008                              
warn_missing    true                                
warn_question   true                                
warn_unchecked  false                               
END



IO_FILE
MODEL TYPE    IO  INTERVAL FILE               
hydro hdf5    out    1hour ${DSM2MODIFIER}.h5  #Flow data to be passed to qual and ptm 
hydro restart out     1mon ${DSM2MODIFIER}.hrf #Initial condition a restart is required
hydro output  out     none ${DSM2MODIFIER}.out #Echoed run status                      
hydro echo    out     none ${DSM2MODIFIER}_hydro_echo.inp  #Echoed input                        
END

GRID
grid_tutorial_base.inp
grid_tutorial_revision.inp
END

INITIAL_CONDITION
ic_tutorial.inp
channel_ic_tutorial.inp
END

HYDRO_TIME_SERIES
input_boundary_hydro_tutorial.inp
input_transfer_flow_tutorial.inp
END

OUTPUT_TIME_SERIES
output_hydro_tutorial.inp
END







