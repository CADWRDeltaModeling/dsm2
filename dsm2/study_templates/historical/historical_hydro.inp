
CONFIGURATION
configuration_historical.inp
END



SCALAR
NAME            VALUE                                
title           "Hydro simulation: ${DSM2MODIFIER}" 
checkdata       false                                 
cont_bad        false                                 
cont_missing    true                                  
cont_question   true                                  
cont_unchecked  true                                  
deltax          5000                                  
display_intvl   1day                                                                  
gravity         32.2                                  
hydro_time_step 15min                                 
levee_slope     0.33                                  
maxiter         20                                    
printlevel      1                                     
theta           0.6                                   
toleranceq      0.001                                 
tolerancez      0.0008                                
warn_missing    true                                  
warn_question   true                                  
warn_unchecked  false                                 
run_start_date  ${START_DATE}                         
run_end_date    ${END_DATE}                           
run_start_time  ${START_TIME}                         
temp_dir        c:/temp
binary_output   false
dss_direct      true
flush_output    20day                              
END



IO_FILE
MODEL TYPE    IO  INTERVAL FILE                          
hydro hdf5    out    1hour ${HYDROTIDEFILE}               #Flow data to be passed to qual and ptm 
hydro restart out     1mon ${HYDRORSTFILE}                #Initial condition a restart is required
hydro output  out     none ${HYDROOUTFILE}                #Echoed run status                      
hydro echo    out     none ${DSM2OUTPUTDIR}/hydro_echo_${DSM2MODIFIER}.inp #Echoed input                           
END



