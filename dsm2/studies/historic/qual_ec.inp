#qual	restart	out  1day	${QUALRSTFILE}    # initial condition in case run must be restarted
END

TIDEFILE
START_DATE    END_DATE     FILENAME
runtime       length      ${HYDROTIDEFILE}
END

OUTPUTPATHS
CHAN     DIST   NAME	 TYPE	INTERVAL PERIOD	 FILENAME
441     LENGTH  RSAC054  STAGE  15MIN   INST     ${QUALOUTDSSFILE}
END
