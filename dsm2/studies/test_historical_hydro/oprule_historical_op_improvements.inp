OPERATING_RULE
NAME        ACTION        TRIGGER
glc_barrier_chg_1999        "SET gate_width(gate=grant_line_barrier, device=barrier) TO 140.0"        "DATETIME == 01JUN1999 OR DATETIME == 01JUN2000"
glc_barrier_full            "SET gate_elev(gate=grant_line_barrier, device=barrier) TO 1.0 WHILE SET gate_width(gate=grant_line_barrier, device=barrier) TO 125.0"        "ts(name = glc_fullpart) >= 1.0"
glc_barrier_partial         "SET gate_elev(gate=grant_line_barrier, device=barrier) TO -13.1 WHILE SET gate_width(gate=grant_line_barrier, device=barrier) TO 77.0"        "ts(name = glc_fullpart) <= 0.0"
mscs_change_boatlock_op     "SET gate_op(gate=montezuma_salinity_control,device=boat_lock,direction=to_node) to ts(name = mtz_bl)"        TRUE
mscs_change_flashboard_op   "SET gate_op(gate=montezuma_salinity_control,device=flashboards,direction=to_node) to ts(name = mtz_fbop)"        TRUE
orhrb_pipes_chg_elev        "SET gate_elev(gate=old_r@head_barrier,device=pipes) TO ts(name = orhrb_pipeelev)"        TRUE
^orhrb_pipes_chg_elev_fall  "SET gate_elev(gate=old_r@head_barrier,device=pipes) TO -5.0"        "DATETIME == 07OCT2000 OR DATETIME == 05OCT2001 OR DATETIME == 03OCT2002"
^orhrb_pipes_chg_elev_spring     "SET gate_elev(gate=old_r@head_barrier,device=pipes) TO -4.0"        "DATETIME == 15APR2000 OR DATETIME == 26APR2001 OR DATETIME == 15APR2002"
END


